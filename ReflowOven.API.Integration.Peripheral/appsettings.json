{
  "Settings": {
    "MaxRecordsInRequest": 30,
    "MaxPageSize": 100,
    "Options": {
      "FilePath": "ReflowOvenAPI_Log.txt",
      "FolderPath": "logs"
    }
  },
  "RaspConfig": {
    "PinsConfig": {
      "LED_STATUS": 2,
      "LED_COMM": 3,
      "PG": 4
    },
    "SerialConfig": {
      "SerialName": "/dev/ttyS0",
      "BaudRate": 115200
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
