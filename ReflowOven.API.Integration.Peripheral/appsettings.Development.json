{
  "Settings": {
    "MaxRecordsInRequest": 30,
    "MaxPageSize": 100,
    "Options": {
      "FilePath": "ReflowOvenAPI_Log.txt",
      "FolderPath": "logs"
    }
  },
  "RaspConfig": {
    "PinsConfig": {
      "LED_STATUS": 6,
      "LED_COMM": 13,
      "PG": 19
    },
    "SerialConfig": {
      "SerialName": "/dev/ttyAMA0",
      "BaudRate": 115200
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
